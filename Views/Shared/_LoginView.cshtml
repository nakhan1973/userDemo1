@using userDemo1.Models;
@{
    SignedInManager UserLoggedIn = null;

    if (Authentication.IsUserLoggedIn())
    {
        UserLoggedIn = userDemo1.Models.Authorization.GetCurrentUser();
    }
}
<div>
    @if (UserLoggedIn == null)
    {
        <p style="margin-left: 35px; margin-top: 16px; color: #9d9d9d;">Anonymous, @Html.ActionLink("Login", "Index", "Home")</p>
    }
    else
    {
        <p style="margin-left: 35px; margin-top: 16px; color: #9d9d9d;">Welcome @UserLoggedIn.User.FirstName @UserLoggedIn.User.LastName, @Html.ActionLink("Logout", "Logout", "Home")</p>
    }
</div>